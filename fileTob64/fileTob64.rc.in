//
// Version
//

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @VEILTOOLS_FULL_VERSION_COMMAS@
 PRODUCTVERSION @VEILTOOLS_FULL_VERSION_COMMAS@
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "Comments", "Object ID to Hex converter."
            VALUE "CompanyName", "TecSec, Inc."
            VALUE "FileDescription", "file 2 hex tool"
			VALUE "FileVersion", "@VEILTOOLS_FULL_VERSION@"
            VALUE "InternalName", "file2hex"
            VALUE "LegalCopyright", "@VEIL_COPYRIGHT@"
            VALUE "OriginalFilename", "file2hex.exe"
            VALUE "ProductName", " file2hex"
			VALUE "ProductVersion", "@VEILTOOLS_FULL_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END


